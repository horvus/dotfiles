/*  Fall 2018 - CEN 111 Intro to Electrical and Computer Engineering *  Final Project: Line-following Robot (Guy-Michel, Andrew) * *  PhotoResistor Sensitivity Test */#define sensorPin	A0int lightThreshold = 300;void setup(){    Serial.begin(9600);  pinMode(sensorPin, INPUT);}void loop(){    int lightLevel = analogRead(sensorPin);    Serial.println(lightLevel);    if (lightLevel < lightThreshold) Serial.println("1");  else Serial.println("0");}